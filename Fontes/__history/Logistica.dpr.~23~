program Logistica;

uses
  Vcl.Forms,
  UCadProduto in 'UCadProduto.pas' {FCadProduto},
  UCadUsuario in 'UCadUsuario.pas' {FCadUsuario},
  UDM in 'UDM.pas' {DM: TDataModule},
  ULogin in 'ULogin.pas' {FLogin},
  Unit1 in 'Unit1.pas' {Form1},
  UPrincipal in 'UPrincipal.pas' {FPrincipal},
  UProdutos in 'UProdutos.pas' {FProdutos},
  UUsuarios in 'UUsuarios.pas' {FUsuarios};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFCadProduto, FCadProduto);
  Application.CreateForm(TFCadUsuario, FCadUsuario);
  Application.CreateForm(TDM, DM);
  Application.CreateForm(TFLogin, FLogin);
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TFPrincipal, FPrincipal);
  Application.CreateForm(TFProdutos, FProdutos);
  Application.CreateForm(TFUsuarios, FUsuarios);
  Application.Run;
end.
